# Configurações do Programa +GEMS
ADMIN_PASSWORD=admin
APP_TITLE=Programa +GEMS
APP_DESCRIPTION=Forje sua lenda, Herói! Acumule GEMS e escreva seu nome no Salão dos Heróis.

# Configurações de ambiente
ENVIRONMENT=development
DEBUG=true

# Caminhos de dados
DATA_PATH=./data
ANEXOS_PATH=./anexos

# Configurações do Streamlit
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost
STREAMLIT_THEME_PRIMARY_COLOR=#6B7E7D
STREAMLIT_THEME_BACKGROUND_COLOR=#FFFFFF

# Configurações de cache
CACHE_TTL=300

# Logs
LOG_LEVEL=INFO
LOG_FILE=gems_program.log
